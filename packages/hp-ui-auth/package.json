{
  "name": "@hp-ui/auth",
  "version": "1.0.0",
  "description": "Core Authentication components for Hospi",
  "source": "src/index.ts",
  "exports": {
    ".": {},
    "./src": {
      "default": "./src/index.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
