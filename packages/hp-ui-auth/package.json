    "url": "https://github.com/solmon/hospi",
    "directory": "packages/hp-ui-auth"
  },
  "dependencies": {
    "@chakra-ui/utils": "^2.0.15",
    "@saas-ui/core": "^2.0.0-next.0",
    "@saas-ui/forms": "^1.5.5",
    "@saas-ui/hooks": "^1.2.2"
  },
  "packageManager": "yarn@3.5.1"
}
